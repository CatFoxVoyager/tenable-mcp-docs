{
  "name": "tenable-api-mcp",
  "version": "1.0.1",
  "description": "MCP server for retrieving Tenable API documentation to assist with script generation",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "watch": "tsc --watch"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "tenable",
    "documentation",
    "api",
    "tenable-io",
    "vulnerability-management"
  ],
  "author": "tenable-api-mcp",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tenable-api-mcp/tenable-api-mcp.git"
  },
  "homepage": "https://github.com/tenable-api-mcp/tenable-api-mcp#readme",
  "bugs": {
    "url": "https://github.com/tenable-api-mcp/tenable-api-mcp/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0-rc.12",
    "turndown": "^7.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/turndown": "^5.0.5",
    "typescript": "^5.7.2"
  },
  "bin": {
    "tenable-api-mcp": "bin/tenable-api-mcp"
  }
}
